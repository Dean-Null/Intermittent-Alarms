<layout id="StartLayout" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://developer.garmin.com/downloads/connect-iq/resources.xsd">
    <bitmap 
        id="ImageCenter" 
        x="center" y="20"
        filename="../drawables/activityIconWorking.png" 
    />
    <label
        id="LabelActivity" 
        x="center" y="140" 
        text="@Strings.Activity" 
        color="Graphics.COLOR_WHITE" 
        justification="Graphics.TEXT_JUSTIFY_CENTER" 
    />
    <label
        id="LabelSequence" 
        x="center" y="210" 
        font="Graphics.FONT_TINY" 
        color="Graphics.COLOR_LT_GRAY" 
        justification="Graphics.TEXT_JUSTIFY_CENTER" 
    />
    <bitmap
        id="LabelLoop" 
        x="center" y="235" 
        filename="../drawables/loopInactive.png" 
    />
    <label 
        id="LabelTimer" 
        x="center" y="330" 
        font="Graphics.FONT_LARGE" 
        color="Graphics.COLOR_WHITE" 
        justification="Graphics.TEXT_JUSTIFY_CENTER" 
        />
</layout>
